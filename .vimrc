syntax on
set t_Co=256
color jellybeans
set nonumber
set nowrap
set ignorecase
set tags=./tags;
set ruler

set tabstop=2
set shiftwidth=2
set softtabstop=2

set smarttab
set autoindent
set expandtab
set smartindent
set cindent

filetype on
set hidden

set noswapfile

au BufRead,BufNewFile *.ros set filetype=php
